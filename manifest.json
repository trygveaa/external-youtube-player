{
  "manifest_version": 2,

  "name": "External youtube player",
  "version": "1.4",

  "content_scripts": [
    {
      "matches": ["http://www.youtube.com/*", "https://www.youtube.com/*"],
      "css": ["youtube-player.css"],
      "js": ["youtube-player.js"],
      "run_at": "document_start"
    }
  ]
}
